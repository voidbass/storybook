{
  "name": "angular-cli",
  "version": "6.0.0-alpha.20",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "prebuild-storybook": "yarn storybook:prebuild",
    "build-storybook": "build-storybook -s src/assets",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "e2e": "ng e2e",
    "ng": "ng",
    "start": "ng serve",
    "storybook": "yarn storybook:prebuild && start-storybook -p 9008 -s src/assets",
    "storybook:prebuild": "yarn test:generate-output && yarn docs:json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --config=jest.addon-config.js --outputFile=addon-jest.testresults.json || true",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "^8.2.8",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@ngrx/store": "^8.2.0",
    "core-js": "^3.0.1",
    "node-sass": "^9.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.4.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "^8.3.6",
    "@angular/compiler-cli": "^8.2.8",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-a11y": "6.0.0-alpha.20",
    "@storybook/addon-actions": "6.0.0-alpha.20",
    "@storybook/addon-backgrounds": "6.0.0-alpha.20",
    "@storybook/addon-centered": "6.0.0-alpha.20",
    "@storybook/addon-docs": "6.0.0-alpha.20",
    "@storybook/addon-jest": "6.0.0-alpha.20",
    "@storybook/addon-knobs": "6.0.0-alpha.20",
    "@storybook/addon-links": "6.0.0-alpha.20",
    "@storybook/addon-options": "6.0.0-alpha.20",
    "@storybook/addon-storyshots": "6.0.0-alpha.20",
    "@storybook/addon-storysource": "6.0.0-alpha.20",
    "@storybook/addons": "6.0.0-alpha.20",
    "@storybook/angular": "6.0.0-alpha.20",
    "@storybook/source-loader": "6.0.0-alpha.20",
    "@types/core-js": "^2.5.0",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.3",
    "@types/webpack-env": "^1.15.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "global": "^4.3.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "^7.1.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.6.2",
    "typescript": "^3.4.0"
  }
}
